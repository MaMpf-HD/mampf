<%= vite_stylesheet_tag "feedbacks/form/_feedback_button.scss" %>

<!-- Feedback button in navbar -->
<%= button_to feedback_form_path,
      method: :get, class: "btn ms-1 me-1",
      id: "feedback-btn", title: t('feedback.tooltip'),
      data: {
        "bs-toggle": "modal",
        "bs-target": "#submit-feedback",
        "turbo_frame": "feedback-form"
      } do %>
  <i class="bi bi-star-fill"></i>
<% end %>
