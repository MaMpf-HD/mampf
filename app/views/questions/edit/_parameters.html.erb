<% if parameters.present? %>
  <hr>
  <div class="row">
    <div class="col-12">
      <h6>
        Parameter
      </h6>
    </div>
  </div>
  <div class="row">
    <% parameters.each do |param, values| %>
      <div class="col-2">
        $<%= param %>$
      </div>
      <div class="col-10">
        $<%= values.class.name == 'Range' ? "[#{values}]" : values.join(',') %>$
      </div>
    <% end %>
  </div>
<% end %>