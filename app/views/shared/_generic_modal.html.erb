<div class="modal fade"
     id="<%= sort %>Modal"
     tabindex="-1"
     role="dialog"
     aria-labelledby="<%=sort %>ModalLabel"
     aria-hidden="true"
     data-backdrop="static">
  <div class="modal-dialog modal-lg"
       role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="<%= sort %>ModalLabel">
          <%= title %>
        </h5>
        <button type="button"
                class="btn-close"
                data-bs-dismiss="modal"
                aria-label="Close">
          <span aria-hidden="true">
            &times;
          </span>
        </button>
      </div>
      <div class="modal-body" id="<%= sort%>-modal-content">
      </div>
    </div>
  </div>
</div>