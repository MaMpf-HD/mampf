<%= render(SearchForm::SearchForm.new(url: search_courses_path, context: "courses")) do |c| %>
  <% c.add_editor_filter %>

  <% c.add_program_filter %>

  <% c.add_fulltext_filter %>

  <% c.add_per_page_filter(
       per_options: hits_per_page(true),
       default: default_hits_per_page(true)
     ) %>

  <% c.add_term_independence_filter %>
<% end %>