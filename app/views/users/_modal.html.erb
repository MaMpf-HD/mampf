<div class="modal fade"
     id="genericUsersModal"
     tabindex="-1"
     role="dialog"
     aria-labelledby="genericUsersModalLabel" aria-hidden="true">
  <div class="modal-dialog modal-lg"
       role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title"
            id="genericUsersModalLabel">
          <%= t('admin.users.edit_generics') %>
        </h5>
        <button type="button"
                class="close"
                data-dismiss="modal"
                aria-label="Close">
          <span aria-hidden="true">
            &times;
          </span>
        </button>
      </div>
      <div class="modal-body"
           id="generic-users-modal-content">
        <%= render partial: "users/generic_users",
                   locals: { generic_users: generic_users } %>
      </div>
      <div class="modal-footer">
        <div class="row">
          <div class="col-12 text-center">
            <button type="button"
                    class="btn btn-sm btn-secondary"
                    data-dismiss="modal">
              <%= t('buttons.close') %>
            </button>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
